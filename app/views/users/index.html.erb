 <%= form_tag(controller: "requests", method: "get") do %>
 <%= text_field_tag :search, params[:search] %>
 <%= submit_tag("search", name: "nil") %>
 <% end %>
 <% if current_user != (@user) %>
 <ol class="microposts">
  <% @users.each do |user| %>  
 <%= paginate @users %>
 <%= render user %>
 <% end %>
 </ol>
 <% end %>
