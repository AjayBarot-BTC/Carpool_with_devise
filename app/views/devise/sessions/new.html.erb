<title>Sign in</title>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <form role="form">
    <div class="form-group">
        <label for="exampleInputEmail1">Email:</label><br>
        <%= f.email_field :email, :autofocus => true, class: "form-control", placeholder: "Enter email", id: "exampleInputEmail1" %>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password:</label><br>
        <%= f.password_field :password, class: "form-control", placeholder: "Password", id: "exampleInputPassword1" %>
    </div>
    <div class="checkbox">
        <label>
        <% if devise_mapping.rememberable? -%>
        <%= f.check_box :remember_me %>Remember me
       <% end %>
      </label>
    </div><br>
      <%= f.submit :Login, class: "btn btn-large btn-primary" %>
  </form>
<% end %><br>
<%= render "devise/shared/links" %>